{
  "name": "immer-relite",
  "version": "0.0.0",
  "description": "Relite + Immer",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "test": "jest --coverage --silent",
    "test:watch": "jest --coverage --watch",
    "build": "tsc",
    "check-type": "tsc --noEmit",
    "format": "prettier --write --parser typescript \"test/**/*.ts?(x)\" \"index.ts\"",
    "prepare": "npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tqma113/immer-relite.git"
  },
  "keywords": [
    "Relite",
    "Immer",
    "Redux"
  ],
  "author": "Ma Tianqi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tqma113/immer-relite/issues"
  },
  "homepage": "https://github.com/tqma113/immer-relite#readme",
  "dependencies": {
    "immer": "^9.0.12"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.8",
    "jest": "^27.4.7",
    "ts-jest": "^27.1.2",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  }
}
