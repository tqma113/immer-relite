{
  "name": "immer-relite",
  "version": "0.0.0",
  "description": "Relite + Immer",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "logger.js",
    "logger.d.ts",
    "attachDevTool.js",
    "attachDevTool.d.ts",
    "react.js",
    "react.d.ts"
  ],
  "scripts": {
    "test": "jest --coverage --silent",
    "test:watch": "jest --coverage --watch",
    "build": "tsc",
    "check-type": "tsc --noEmit",
    "format": "prettier --write --parser typescript \"test/**/*.ts?(x)\" \"src/*.ts\" \"src/**/*.ts\" \"src/**/*.tsx\"",
    "prepare": "npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tqma113/immer-relite.git"
  },
  "keywords": [
    "Relite",
    "Immer",
    "Redux"
  ],
  "author": "Ma Tianqi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tqma113/immer-relite/issues"
  },
  "homepage": "https://github.com/tqma113/immer-relite#readme",
  "peerDependencies": {
    "react": "> 16.8.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "dependencies": {
    "immer": "^9.0.12",
    "use-sync-external-store": "1.0.0-beta-24dd07bd2-20211208"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.8",
    "@types/react": "^17.0.38",
    "@types/use-sync-external-store": "^0.0.3",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "ts-jest": "^27.1.2",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  }
}
